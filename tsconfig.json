{
    "compilerOptions": {
        "target": "es2017",
        "lib": [
            "ES6"
        ],
        "module": "commonjs",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "removeComments": true
    }
}
